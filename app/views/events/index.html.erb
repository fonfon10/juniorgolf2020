<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tour</th>
      <th>Course</th>
      <th>Gender boy</th>
      <th>Gender girl</th>
      <th>Category jun</th>
      <th>Category juv</th>
      <th>Category ban</th>
      <th>Category pee</th>
      <th>Category ato</th>
      <th>Level</th>
      <th>Days</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Reg deadline</th>
      <th>Qual required</th>
      <th>Comments</th>
      <th>User</th>
      <th>Private</th>
      <th>Mother</th>
      <th>Father</th>
      <th>Other children</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.tour_id %></td>
        <td><%= event.course_id %></td>
        <td><%= event.gender_boy %></td>
        <td><%= event.gender_girl %></td>
        <td><%= event.category_jun %></td>
        <td><%= event.category_juv %></td>
        <td><%= event.category_ban %></td>
        <td><%= event.category_pee %></td>
        <td><%= event.category_ato %></td>
        <td><%= event.level_id %></td>
        <td><%= event.days %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.reg_deadline %></td>
        <td><%= event.qual_required %></td>
        <td><%= event.comments %></td>
        <td><%= event.user_id %></td>
        <td><%= event.private %></td>
        <td><%= event.mother %></td>
        <td><%= event.father %></td>
        <td><%= event.other_children %></td>
        <td><%= event.note %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
